{
  "manifest_version": 2,

  "name": "SiteWait",
  "description": "Postpone your guilty pleasure.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": false
  },
  "permissions": [
    "webNavigation",
    "https://*/",
    "http://*/",
    "tabs",
    "activeTab",
    "storage"
  ],
  "web_accessible_resources": [
    "Warning_sign.png"
  ]
}
